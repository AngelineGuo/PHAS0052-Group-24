import requests
import os
import csv

### website requests

atpbind = 'https://zhanggroup.org/ATPbind/result_example/index.html'
pdb_id = []
with open('PHAS0052 PDB File - Sheet1.csv') as csvDataFile:
    read = csv.reader(csvDataFile)
    for row in read:
        pdb_id.append(row[0])
print(pdb_id)

